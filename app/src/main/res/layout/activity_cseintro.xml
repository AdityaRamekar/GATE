<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="82dp"
    tools:layout_editor_absoluteY="160dp">
    tools:context=".contents">

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/textview10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="0"
            android:text="Video Lectures"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold" />
    <TextView
        android:id="@+id/Part1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:text="PART 1: VERBAL ABILITY"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:textSize="20sp" />

        <TextView
            android:id="@+id/eg"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:layout_weight="0"
            android:layout_margin="5dp"
            android:background="@drawable/rounded_corner"
            android:text="English Grammar"
            android:textAlignment="center"
            android:textColor="@color/teal_200"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/sc"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:layout_weight="0"
            android:layout_margin="5dp"
            android:background="@drawable/rounded_corner"
            android:text="Sentence Completion"
            android:textColor="@color/teal_200"
            android:textSize="15sp" />
    <TextView
        android:id="@+id/synonyms"
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:layout_height="45dp"
        android:layout_weight="0"
        android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
        android:text="Synonyms"
        android:clickable="true"
        android:layout_gravity="center"
        android:textColor="@color/teal_200"
        android:textSize="15sp"
        />
    <TextView
            android:id="@+id/antonyms"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_weight="0"
            android:textAlignment="center"
            android:text="Antonyms"
            android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
            android:clickable="true"
            android:layout_gravity="center"
            android:textColor="@color/teal_200"
            android:textSize="15sp"
            />
    <TextView
        android:id="@+id/reason"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textAlignment="center"
        android:layout_weight="0"
        android:text="Reasoning Ability"
        android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
        android:textColor="@color/teal_200"
        android:layout_gravity="center"
        android:textSize="15sp"
        />
    <TextView
        android:id="@+id/Part2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:layout_weight="0"
        android:text="PART 2: NUMERICAL ABILITY"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/numbers"
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:layout_height="45dp"
        android:layout_weight="0"
        android:text="Numbers and Algebra"
        android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
        android:textColor="@color/teal_200"
        android:layout_gravity="center"
        android:textSize="15sp"
        />
    <TextView
        android:id="@+id/percent"
        android:layout_width="match_parent"
        android:textAlignment="center"
        android:layout_height="45dp"
        android:layout_weight="0"
        android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
        android:text="Percentage and Its Applications"
        android:textColor="@color/teal_200"
        android:layout_gravity="center"
        android:textSize="15sp"
      />
    <TextView
        android:id="@+id/time"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textAlignment="center"
        android:layout_weight="0"
        android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
        android:text="Time and Work"
        android:textColor="@color/teal_200"
        android:layout_gravity="center"
        android:textSize="15sp"
        />
    <TextView
        android:id="@+id/ratio"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textAlignment="center"
        android:layout_weight="0"
        android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
        android:text="Ratio, Proportion and Mixtures"
        android:textColor="@color/teal_200"
        android:layout_gravity="center"
        android:textSize="15sp"
        />
    <TextView
        android:id="@+id/pnc"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:textAlignment="center"
        android:layout_weight="0"
        android:layout_margin="5dp"
        android:background="@drawable/rounded_corner"
        android:text="Permutations and Combinations &amp; Probability"
        android:textColor="@color/teal_200"
        android:layout_gravity="center"
        android:textSize="15sp"
       />
        <TextView
            android:id="@+id/misc"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:textAlignment="center"
            android:layout_weight="0"
            android:layout_margin="5dp"
            android:background="@drawable/rounded_corner"
            android:text="Miscellaneous"
            android:textColor="@color/teal_200"
            android:layout_gravity="center"
            android:textSize="15sp"
            />

        <TextView
            android:id="@+id/textview9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:layout_gravity="center"
            android:layout_weight="0"
            android:background="#E40E0E"
            android:text="Theory notes"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="15sp" />

        <Button
            android:id="@+id/opennotes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Open Notes"
            android:onClick="displayToast"
            android:textColor="@color/white"
            app:backgroundTint="@color/material_on_background_disabled" />
    </LinearLayout>
</ScrollView>